<fieldset disabled>
	<legend>Merging Report</legend>
	<div id="merge-summary" class="form-horizontal disabled">
   	<div class="form-group" >
    	<label for="graph" class="col-sm-2 control-label">Graph</label>
     	<div class="col-sm-10 " >
       		<input id="graph" class="form-control" type="text" value="{{commit.graphName}}"></input>
     	</div>
   	</div>
   	<div class="form-group" >
    	<label for="strategy" class="col-sm-2 control-label">Strategy</label>
     	<div class="col-sm-10 " >
			<input id="strategy" class="form-control" type="text" value="{{commit.strategy}}"></input>
     	</div>
   	</div>
	<div class="form-group" >
		<label class="col-sm-2 control-label">SDD</label>
	    <div class="col-sm-10 ">          
	    	<input type="text" class="form-control" value="{{commit.sddName}}"></input>
       	</div>
   	</div>
   	<div class="form-group">
    	<label class="col-sm-2 control-label">Merge</label>
    	<div class="col-sm-4 ">
	    	<input type="text" class="form-control" value="{{commit.branch1}}"></input>        
     	</div>
     	<label class="col-sm-2 small-offset-2 control-label">Into</label>
	    <div class="col-sm-4 ">
        	<input type="text" class="form-control" value="{{commit.branch2}}"></input>         
     	</div>
   	</div>
	<div class="form-group">
    	<label class="col-sm-2 control-label">User</label>
       	<div class="col-sm-10 ">
        	<input type="text" class="form-control" value="{{commit.user}}" id="user" name="user"></input>
       	</div>
    </div>
    <div class="form-group">
    	<label class="col-sm-2 control-label">Timestamp</label>
       	<div class="col-sm-10 ">
        	<input type="text" class="form-control" value="{{commit.timestamp}}" id="user" name="user"></input>
       	</div>
    </div>    
	<div class="form-group">
		<label class="col-sm-2 control-label">Message</label>
	    <div class="col-sm-10 ">
	    	<textarea class="form-control" rows="6" id="message" name="message" value="{{commit.message}}"></textarea>
	    </div>
	   </div>	  
   	</div>
 </fieldset>